<!DOCTYPE html>
<html>
<head>
	<title>Index Page</title>
</head>
<body>
	<h1>Listing Users</h1>
	<a href="users/new">Create New User</a>
	<table border="1">
		<tr>
			<th>first name</th>
			<th>last name</th>
			<th>Email</th>
			<th>Actions</th>
		</tr>
		<% @users.each do |user| %>
		<tr>
			<td><%=user.first_name%></td>
			<td><%=user.last_name%></td>
			<td><%=user.email_address%></td>
			<td><a href="users/<%=user.id%>">Show</a>|<a href="users/<%=user.id%>/edit"> Edit </a><a href="users/<%=user.id%>"data-method="delete">Destroy</a>
		</tr>
		<%end%>
	</table>
</body>
</html>