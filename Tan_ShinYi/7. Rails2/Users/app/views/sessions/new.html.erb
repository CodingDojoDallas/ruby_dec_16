<h1>Login</h1>
<ul>
  <% flash.each do |key, value| %>
      <li><%= value %></li>
  <% end %>
</ul>
<form action='/sessions/log_in' method='post'>
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
  Email: <input type="email" name="user[email]" placeholder="Email"/></br>
  Password: <input type="password" name="user[password]" placeholder="Password" /></br>
  <input type='submit' value='Log In'>
</form>
<a href="/">Back</a>
